{
  "extends": ["eslint:recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6
  },
  "rules": {
    "valid-jsdoc": 2,
    "comma-dangle": 0,
    "no-unused-vars": 1,
    "no-console": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}
