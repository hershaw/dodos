  },
  "homepage": "https://github.com/nfcampos/dodos",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-require": "^1.0.1",
    "eslint": "~2.3.0",
    "lodash": "^4.6.1",
    "nodemon": "^1.9.1",
    "npm-check": "^4.1.4",
    "tap": "^5.7.0",
